{
    "compilerOptions": {
        "strict": true,
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "skipLibCheck": true,
        "paths": {
            "@/components/*": ["./components/*"],
            "@/server-types/*": ["../server/types/*"],
            "@/*": ["./*"],
        }
    }
}
